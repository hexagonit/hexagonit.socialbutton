<?xml version="1.0"?>
<object
  i18n:domain="hexagonit.socialbutton"
  name="portal_controlpanel"
  xmlns:i18n="http://xml.zope.org/namespaces/i18n"
  >

  <configlet
    action_id="hexagonit_socialbutton_code"
    appId="hexagonit.socialbutton"
    category="Products"
    condition_expr=""
    i18n:attributes="title"
    icon_expr="string:$portal_url/maintenance_icon.png"
    title="Social Button Code Setting"
    url_expr="string:${portal_url}/@@social-button-code-controlpanel"
    visible="True"
    >
    <permission>Manage portal</permission>
  </configlet>

  <configlet
    action_id="hexagonit_socialbutton_config"
    appId="hexagonit.socialbutton"
    category="Products"
    condition_expr=""
    i18n:attributes="title"
    icon_expr="string:$portal_url/maintenance_icon.png"
    title="Social Button Configuration"
    url_expr="string:${portal_url}/@@social-button-config-controlpanel"
    visible="True"
    >
    <permission>Manage portal</permission>
  </configlet>

</object>